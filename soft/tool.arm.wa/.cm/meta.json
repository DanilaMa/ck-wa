{
  "auto_detect": "yes",
  "customize": {
    "check_that_exists": "yes",
    "ck_version": 10,
    "env_prefix": "CK_ENV_TOOL_ARM_WA",
    "limit_recursion_dir_search": {
      "linux": 3
    },
    "only_for_host_os_tags": [
      "linux"
    ],
    "only_for_target_os_tags": [
      "linux",
      "android"
    ],
    "soft_file": {
      "linux": "wa"
    },
    "soft_version_cmd": {
      "linux": "--version > $#filename#$ 2>&1"
    }
  },
  "deps": {
    "python": {
      "local": "yes",
      "name": "Python compiler (v2.x)",
      "sort": 10,
      "tags": "compiler,lang-python"
    }
  },
  "soft_name": "ARM workload automation tool",
  "tags": [
    "tool",
    "arm",
    "wa"
  ]
}
